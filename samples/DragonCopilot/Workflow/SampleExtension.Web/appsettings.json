{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "Name": "Sample Extension API",
    "Version": "1.0.0",
    "Description": "A sample extension API that processes requests from the Dragon Backend Simulator"
  },
  "Authentication": {
    "Enabled": true,
    "TenantId": "<your-tenant-id>",
    "ClientId": "<your-client-id>",
    "Instance": "https://login.microsoftonline.com/",
    "MapInboundClaims": false,
    "RequiredClaims":{
      "oid": ["12345678-1234-1234-1234-123456789012"]
    }
  },
  "LicenseKey": {
    "Enabled": true,
    "HeaderName": "X-License-Key",
    "ValidKeys": ["premium-key-1", "enterprise-key-2", "partner-key-3"]
  }
}
